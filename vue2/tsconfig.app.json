{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "declare/env.d.ts",
    "src/**/*",
    "src/**/*.d.ts",
    "src/*/*.d.ts",
    "src/**/*.vue",
    "declare/auto-imports.d.ts"
  ],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
